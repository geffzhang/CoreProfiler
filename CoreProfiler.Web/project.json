{
  "version": "1.0.3",
  "description": "CoreProfiler.Web - A .NET Core Profiling Library",
  "authors": [ "teddy.ma" ],
  "dependencies": {
    "CoreProfiler": "1.0.3",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0",
    "Newtonsoft.Json": "9.0.1",
    "System.Net.Http": "4.1.0",
    "System.Text.Encoding.Extensions": "4.0.11",
    "System.Xml.XmlDocument": "4.0.1",
    "System.ServiceModel.Primitives": "4.1.0",
    "System.ServiceModel.Http": "4.1.0"
  },
  "buildOptions": {
    "emitEntryPoint": false,
    "embed": {
      "include": "icons.png;treeview_timeline.css",
      "mappings": {
        "Web/icons.png": "icons.png",
        "Web/treeview_timeline.css": "treeview_timeline.css"
      }
    }
  },
  "packOptions": {
    "title": "CoreProfiler.Web",
    "tags": [ "coreclr", "profiling", ".netcore", "coreprofiler", "miniprofiler", "nanoprofiler", "performance", "timing", "profiler" ],
    "projectUrl": "https://github.com/teddymacn/coreprofiler",
    "licenseUrl": "https://mit-license.org/"
  },
  "frameworks": {
    "netstandard1.3": {
      "imports": [
        "portable-dnxcore50+net45+win8+wp8+wpa81"
      ]
    }
  }
}
